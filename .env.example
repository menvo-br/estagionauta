
# ===========================
# SUPABASE CONFIGURATION
# ===========================
# Obtenha essas chaves no dashboard do Supabase: https://supabase.com/dashboard
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ===========================
# OPENAI CONFIGURATION
# ===========================
# Para análise de currículo com IA
# Obtenha em: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=your_openai_api_key_here

# ===========================
# STRIPE CONFIGURATION (Opcional)
# ===========================
# Para pagamentos de análises e assinaturas
# Obtenha em: https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# ===========================
# APP CONFIGURATION
# ===========================
# URL base da aplicação
VITE_APP_URL=http://localhost:8080

# ===========================
# MAPBOX CONFIGURATION (Opcional)
# ===========================
# Para mapas interativos das agências
# Obtenha em: https://mapbox.com/
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here

# ===========================
# ANALYTICS (Opcional)
# ===========================
# Google Analytics
VITE_GA_ID=your_google_analytics_id_here

# Hotjar ou similar
VITE_HOTJAR_ID=your_hotjar_id_here

# ===========================
# SENTRY (Opcional)
# ===========================
# Para monitoramento de erros
VITE_SENTRY_DSN=your_sentry_dsn_here

# ===========================
# FEATURE FLAGS (Opcional)
# ===========================
# Para ativar/desativar funcionalidades gradualmente
VITE_FEATURE_PAYMENTS_ENABLED=false
VITE_FEATURE_MAPS_ENABLED=false
VITE_FEATURE_EMAIL_SHARING=false
VITE_FEATURE_ADMIN_PANEL=false

# ===========================
# DEVELOPMENT ONLY
# ===========================
# Para desenvolvimento local
NODE_ENV=development
PORT=8080

# ===========================
# INSTRUÇÕES DE CONFIGURAÇÃO
# ===========================
# 1. Copie este arquivo para .env.local
# 2. Preencha as variáveis necessárias
# 3. Configure OAuth no Supabase Dashboard
# 4. Execute: npm run dev

# ===========================
# LINKS ÚTEIS
# ===========================
# Supabase Dashboard: https://supabase.com/dashboard
# OpenAI API Keys: https://platform.openai.com/api-keys
# Stripe Dashboard: https://dashboard.stripe.com
# Google Cloud Console: https://console.cloud.google.com
# Mapbox Account: https://account.mapbox.com
